{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "NCacheSettings": {
    "CacheName": "myClusteredCache",
    "EnableLogs": true,
    "EnableDetailLogs": false,
    "ExceptionsEnabled": true,
    "OperationRetry": 0,
    "operationRetryInterval": 0
  }
}
